<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <AssemblyName>Lykke.HftApi.ApiContract</AssemblyName>
    <PackageId>Swisschain.Lykke.HftApi.ApiContract</PackageId>
    <RootNamespace>Lykke.HftApi.ApiContract</RootNamespace>
    <Authors>Swisschain</Authors>
    <Product>Lykke</Product>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.19.1" />
    <PackageReference Include="Grpc.Net.Client" Version="2.40.0" />
    <PackageReference Include="Grpc.Tools" Version="2.41.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <Protobuf Include="common.proto" />
    <Protobuf Include="isalive.proto" />
    <Protobuf Include="privateService.proto" />
    <Protobuf Include="publicService.proto" />
  </ItemGroup>

</Project>
